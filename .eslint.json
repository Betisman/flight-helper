{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "quotes": [
      "error",
      "single"
    ],
    "indent": [
      "error",
      2
    ]
  }
}